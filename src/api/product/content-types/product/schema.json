{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid"
    },
    "Description": {
      "type": "blocks"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "catagory": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::catagory.catagory",
      "inversedBy": "products"
    },
    "seoMeta": {
      "displayName": "seo",
      "type": "component",
      "repeatable": false,
      "component": "product.seo"
    },
    "price": {
      "type": "biginteger"
    },
    "size": {
      "displayName": "size",
      "type": "component",
      "repeatable": true,
      "component": "size.size"
    }
  }
}
